import sys
from collections import deque

input = sys.stdin.readline

N, K, M = map(int, input().split())

arr = deque(range(1, N + 1))
answer = []
cnt = 0

while True:
    if len(arr) == 0:
        break
    arr.rotate(-K)
    answer.append(arr.pop())
    cnt += 1
    if cnt == M:
        arr.reverse()
        cnt = 0

for i in answer:
    print(i, end="\n")
